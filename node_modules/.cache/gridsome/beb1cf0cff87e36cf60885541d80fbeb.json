e.category.title }}</h1>\n\n            <div class=\"w-full sm:flex text-xl text-gray-600 sm:mx-auto\">\n                <div class=\"h-auto border-b sm:border-b-0 sm:border-r border-primary-200\">\n                    <categories class=\"list\"/>\n                </div>\n                <div class=\"w-full px-auto p-4\">\n                    <apps :category=$page.category.belongsTo.apps />\n                </div>\n            </div>\n\n        </div>\n    </Layout>\n</template>\n\n<page-query>\n    query Category ($id: ID!) {\n    category: category (id: $id) {\n    title\n    belongsTo{\n    totalCount\n    pageInfo {\n    totalPages\n    currentPage\n    }\n    apps:edges {\n    node {\n    ...on App {\n    title\n    path\n    summary\n    thumbnail\n    url\n    }\n    }\n    }\n    }\n    }\n    }\n</page-query>\n\n<script>\n    import Apps from '../layouts/Apps'\n    import Categories from '../layouts/Categories'\n\n\n    export default {\n        metaInfo: {\n            title: `Tools`\n        },\n        components: {\n            Apps, Categories\n        }\n    }\n</script>\n\n"],"sourceRoot":"src/templates"}]}